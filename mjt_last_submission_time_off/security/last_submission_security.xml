<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
		<record model="ir.module.category" id="access_right_category">
			<field name="name">Last Submission Time Off</field>
			<field name="description">Access right to Asset</field>
		</record>

		<record model="res.groups" id="mjt_last_submission_time_off.group_employee">
			<field name="name">User</field>
			<field name="category_id" ref="access_right_category"/>
		</record>

		<record model="res.groups" id="mjt_last_submission_time_off.group_admin">
			<field name="name">Administrator</field>
			<field name="category_id" ref="access_right_category"/>
			<field name="implied_ids" eval="[(4, ref('mjt_last_submission_time_off.group_employee'))]"/>
		</record>
	</data>
</odoo>